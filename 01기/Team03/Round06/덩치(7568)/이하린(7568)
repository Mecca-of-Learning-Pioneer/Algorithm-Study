#include <iostream>
#include <vector>
using namespace std;

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);
    cout.tie(nullptr);

    vector<pair<int,int>> person;
    int N;
    cin>>N;
    int tall, weight;
    for(int i=0; i<N;i++){
        cin>>tall>>weight;
        person.push_back(make_pair(tall, weight));
    }

    for(int i=0;i<N;i++){
        int bigger=1;
        for(int j=0; j<N; j++){
            if(i==j) continue;
            if(person[j].first>person[i].first && person[j].second>person[i].second)
                bigger++;
        }
        if(i==N-1)
            cout<<bigger;
        else cout<<bigger<<" "; //공백차이 

    }

} 

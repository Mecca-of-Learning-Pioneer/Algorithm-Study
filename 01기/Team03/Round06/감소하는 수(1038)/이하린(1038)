#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

vector<long long>v;

void track(string s){
    string temp=s;
    reverse(temp.begin(), temp.end());
    v.push_back(stoll(temp));
    for(int i=s.back()-'0'+1; i<=9; i++){
        string num=to_string(i);
        track(s+num);
    }

}

int main(){
    int n;
    cin>>n;
    if(n>1022) {cout<<"-1"; return 0;}

    for(int i=0;i<=9;i++){
        string num=to_string(i);
        track(num);
    }
    sort(v.begin(), v.end());

    cout<<v[n];
}
